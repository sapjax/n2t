/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=b, out=ab);
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=notA, b=notB, out=notAB);
    Or(a=ab, b=notAB, out=aSameAsB);
    Not(in=aSameAsB, out=out);
}